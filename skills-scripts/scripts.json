[
  {
    "id": "script-ateb-id",
    "command": "hola",
    "description": "Script de Bienvenida",
    "script": [
      {
        "topic": "default",
        "script": [
          {
            "text": [
              "Hola! ¿Cómo estás?"
            ]
          },
          {
            "text": [
              "¿Te puedo ayudar a localizar alguno de nuestros productos?"
            ],
            "collect": null
          },
          {
            "action": "complete"
          },
          {
            "text": [
              "Tenemos un catálogo con una gran variedad de productos"
            ],
            "quick_replies": [
              {
                "title": "Ver Productos y Servicios",
                "payload": "productos",
                "content_type": "text"
              }
            ]
          }
        ]
      },
      {
        "topic": "on_timeout",
        "script": [
          {
            "text": [
              "Looks like you got distracted. We can continue later."
            ]
          },
          {
            "action": "timeout"
          }
        ]
      }
    ],
    "triggers": [
      {
        "pattern": ".*buen(a?s?)(os)? (d[iíÍ]as?|tardes?|noches?).*",
        "type": "string",
        "id": 269
      },
      {
        "pattern": ".*hola.*",
        "type": "regexp",
        "id": 196
      }
    ],
    "variables": [
      {
        "name": "question_1",
        "type": "string"
      },
      {
        "name": "question_2",
        "type": "string"
      },
      {
        "name": "question_3",
        "type": "string"
      }
    ]
